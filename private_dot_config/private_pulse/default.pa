# Get system config
.include /etc/pulse/default.pa
.nofail
load-module module-echo-cancel sink_name=echo_cancel_sink source_name=echo_cancel_source use_master_format=true rate=48000 aec_method=webrtc aec_args="voice_detection=1 analog_gain_control=0 digital_gain_control=1"

set-default-source echo_cancel_source
set-default-sink echo_cancel_sink
